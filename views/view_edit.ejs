	<div id="features" class="clearfix side" style="background: -webkit-linear-gradient(left, #b5daae, #a8cf45, #b5daae);">
        <div style="margin-left: 5%;margin-right:5%; margin-top: 1%;margin-bottom: 0.5%;background-color: whitesmoke" class="col-sm-11 " >    
        <div class="row1" class="clearfix"  style="margin-top: 2%;margin-bottom: 2%;margin-left: 5%;margin-right: 5%">
                <div class="row" >
                        <% 
                        for(var i=0; i < result.length ; i++)
                        {   
                        %>
                        <form method="POST" action="/save/warehouse?id=+<%=result[0].w_id%>"  enctype="multipart/form-data">
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <div class="service-widget">
                                <div class="property-main">
                                    <div class="property-wrap">
                                        <figure class="post-media wow fadeIn">
                                            <img src="<%=result[i].w_image %>" style="height: 100%;width:100%" alt="" class="img-responsive">
                                        </figure>
                                        <div class="item-foot">
                                            <input type="file" name="image">
                                        </div>
                                    </div>
                                </div>
                            </div><!-- end service -->
                        </div>
                        <div class="col-md-5" style="background-color: white;margin-top: 1%;padding: 1%;">
                            <input name="warehouse_title" type="text" style="border-bottom: 1px solid rgb(170, 170, 170);width: 100%;" value="<%= result[i].w_propertyTitle  %>">
                                        
                             <p style="margin-top:2%;font-weight:bold">Description:</p>
                            <p class="text-center"><textarea style="width: 100%" name="warehouse_description"><%= result[i].w_propertyDescription%></textarea></p>
                         <p style="font-weight:bold">Address:</p>
                      <p class="text-center"><textarea style="width: 100%" name="warehouse_address"><%= result[i].w_address%></textarea></p>
                    
                           <table>
                               <tr style="margin-top:1%">
                                    <td>
                                     <p style="font-weight:bold">Phone Number</p>
                                    </td>
                                    <td ><p style="font-weight:bold">:</p></td>
                                      <td>
                                         <p ><input type="text" value="<%= result[i].w_phone %>" name="warehouse_phone"></p>
                                    </td>
                                </tr>
                                <tr style="margin-top:1%">
                                        <td>
                                         <p style="font-weight:bold">Payment Type</p>
                                        </td>
                                        <td ><p style="font-weight:bold">:</p></td>
                                            <td>
                                             <p> <%
                                                 if(result[i].w_paymentType == 1)
                                                 {
                                                     %><%="Monthly"%><%
                                                 }
                                                 else if(result[i].w_paymentType ==2 )
                                                 {
                                                    %><%="Yearly"%><%
                                                 }
                                                 else if(result[i].w_paymentType ==3 )
                                                 {
                                                    %><%="Weekly"%><%
                                                 }
                                                 else{
                                                    %><%="Undefined"%><%
                                                 } %></p>
                                        </td>
                                    </tr>
                                    <tr style="margin-top:1%">
                                            <td>
                                             <p style="font-weight:bold">Rent</p>
                                            </td>
                                            <td ><p style="font-weight:bold">:</p></td>
                                                <td>
                                                 <p><input type="text"  value="<%= result[i].w_rent %>" name="warehouse_rent" ></p>
                                            </td>
                                        </tr>
                            </table>
                            <input class="btn btn-success btn-md" style="width: 50%;margin-left: 25%;" value="Save Edit" type="submit"> 
                        </div>
                    </form>
                        <%
                        }
                        %>

                    </div><!-- end row -->
        </div>
        </div>
	
</div><!-- end section -->
