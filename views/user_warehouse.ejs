	<div class="all-title-box">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h2>User Profile</h2>
					<!-- Breadcrumbs -->
					<nav id="breadcrumbs">
						<ul>
							<li><a href="/">Home</a></li>
							<li>Profile</li>
						</ul>
					</nav>
				</div>
			</div>
		</div>
	</div>
	<div id="features" class="clearfix side" style="background: -webkit-linear-gradient(left, #b5daae, #a8cf45, #b5daae);">
            <div id="sidebar"  class="col-sm-3 " style="background-color: #333;margin-top: 1%">
                    <div id="navbar" class="navbar-collapse ">
                            <ul style="margin-bottom: 2%;" class="nav navbar-nav navbar-right">
                                <li ><a href="/pro" >My Profile</a></li>
                                <li ><a href="/userWarehouses" >My Warehouses</a></li>
                                <li><a  href="/bookings" >Booked Warehouses</a></li>
                                <li ><a href="/userWarehouses" >Pending Payments</a></li>
                                <li ><a href="/profile/reports" >View Complains</a></li>
                            </ul>
                        </div>   
            </div>
        <div style="margin-left: 1%;margin-top: 1%;margin-bottom: 0.5%;background-color: whitesmoke" class="col-sm-8 " >    
        <div class="row1" class="clearfix" style="margin-top: 2%;margin-bottom: 2%;margin-left: 5%;margin-right: 5%">
                <div class="row">
                        <%
                        if(result.length == 0)
                        {
                            %><p> No data found </p> <%
                        } 
			for(var i=0; i < result.length ; i++)
			{   
            %>
            
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <div class="service-widget">
                        <div class="property-main">
                            <div class="property-wrap">
                       
                                <figure class="post-media wow fadeIn">
                                    <a href="/view?id=<%=result[i].w_id%>"  class="global-radius">
                                    <img src="<%=result[i].w_image %>" style="height: 200px;object-fit: cover;" alt="" class="img-responsive">
                                </a>
                                    <div class="label-inner">
                                            <%
                                            if(result[i].w_statusOfBooking == 0)
                                            {
                                            %>
                                            <span class="label-status label ">
                                            <%="Avilable"%>
                                            </span>
                                            <%
                                            }
                                            else if(result[i].w_statusOfBooking == 1)
                                            {
                                            %>
                                            <span class="label-status label label-danger">
                                            <%="Not Avilable"%>
                                            </span>
                                            <%
                                            }
                                            else
                                            {
                                            %>
                                            <span class="label-status label label-warning">
                                            <%="Invalid- Report"%>
                                            </span>
                                            <%
                                            }
                                            %>

                                    </div>
                                    <div class="price">
                                        <span class="item-sub-price"><%=result[i].w_rent %></span> 
                                    </div>
                                </figure>
                                <div class="item-body">
                                    
                                    <div class="info">
                                        <p><%=result[i].w_propertyTitle%></p>
                                    </div>
                                    <div class="adderess">
                                        <i class="fa fa-map-pin" aria-hidden="true"></i> 
                                        <%=result[i].w_propertyDescription%>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </a>
                    </div><!-- end service -->
                </div>
            
			<%
			}
			%>
                    </div><!-- end row -->
        </div>
        </div>
	
</div><!-- end section -->
